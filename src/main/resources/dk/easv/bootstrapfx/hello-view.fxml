<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.bootstrapfx.HelloController">
   <children>
      <VBox alignment="CENTER" spacing="20.0">
          <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </padding>
          <Button onAction="#onHelloButtonClick" styleClass="btn, btn-lg, btn-info" text="Hello!" />

         <Label styleClass="h1" text="Label h1" />
         <Label styleClass="h2" text="Label h2" />
         <Label styleClass="h3" text="Label h3" />
         <Label styleClass="h4" text="Label h4" />
         <Label styleClass="h5" text="Label h5" />
         <Label styleClass="p, b" text="Label p" />
      </VBox>
      <HBox fx:id="hboxSnackbarP" alignment="BOTTOM_CENTER" maxHeight="50.0" maxWidth="200.0" prefHeight="50.0" prefWidth="200.0" visible="false" StackPane.alignment="BOTTOM_CENTER" disable="true" />
   </children>
</StackPane>
